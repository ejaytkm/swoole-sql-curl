{
    "__inputs": [
        { "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus", "pluginName": "Prometheus" }
    ],
    "__requires": [
        { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.0.0" },
        { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
        { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
        { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
        { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "" }
    ],
    "title": "Swoole Jobs (ss_*)",
    "uid": "SS_JOBS_DASH",
    "timezone": "",
    "schemaVersion": 36,
    "refresh": "5s",
    "time": { "from": "now-30m", "to": "now" },
    "panels": [
        {
            "id": 1, "type": "stat", "title": "Jobs OK (5m)",
            "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
            "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
            "targets": [{ "expr": "sum(increase(ss_jobs_jobs_ok[5m]))", "instant": true, "refId": "A" }]
        },
        {
            "id": 2, "type": "stat", "title": "Jobs ERR (5m)",
            "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
            "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
            "targets": [{ "expr": "sum(increase(ss_jobs_jobs_err[5m]))", "instant": true, "refId": "A" }]
        },
        {
            "id": 3, "type": "timeseries", "title": "Job p95 (s)",
            "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
            "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
            "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(ss_jobs_duration_seconds_bucket[5m])) by (le))", "refId": "A", "legendFormat": "p95" }],
            "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
        },
        {
            "id": 4, "type": "timeseries", "title": "Throughput by module",
            "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
            "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
            "targets": [
                { "expr": "sum(rate(ss_jobs_jobs_ok[1m])) by (module)", "refId": "A", "legendFormat": "{{module}} OK/s" },
                { "expr": "sum(rate(ss_jobs_jobs_err[1m])) by (module)", "refId": "B", "legendFormat": "{{module}} ERR/s" }
            ]
        },
        {
            "id": 5, "type": "bargauge", "title": "Workers (RR-style, approximated)",
            "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
            "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
            "targets": [
                { "expr": "ss_jobs_total_workers",   "refId": "A", "legendFormat": "total" },
                { "expr": "ss_jobs_workers_ready",   "refId": "B", "legendFormat": "ready" },
                { "expr": "ss_jobs_workers_working", "refId": "C", "legendFormat": "working" },
                { "expr": "ss_jobs_workers_invalid", "refId": "D", "legendFormat": "invalid" }
            ]
        },
        {
            "id": 6, "type": "bargauge", "title": "Totals by module",
            "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
            "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
            "targets": [
                { "expr": "sum(ss_jobs_jobs_ok) by (module)",  "refId": "A", "legendFormat": "{{module}} OK" },
                { "expr": "sum(ss_jobs_jobs_err) by (module)", "refId": "B", "legendFormat": "{{module}} ERR" }
            ]
        }
    ],
    "templating": { "list": [] }
}