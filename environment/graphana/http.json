{
  "__inputs": [
    { "name": "DS_PROMETHEUS", "label": "Prometheus", "type": "datasource", "pluginId": "prometheus", "pluginName": "Prometheus" }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "" }
  ],
  "title": "Swoole HTTP (ss_*)",
  "uid": "SS_HTTP_DASH",
  "timezone": "",
  "schemaVersion": 36,
  "refresh": "5s",
  "time": { "from": "now-30m", "to": "now" },
  "panels": [
    {
      "id": 1, "type": "stat", "title": "Uptime",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "targets": [{ "expr": "ss_http_uptime_seconds", "instant": true, "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    },
    {
      "id": 2, "type": "stat", "title": "In-flight",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "targets": [{ "expr": "ss_http_requests_inflight", "instant": true, "refId": "A" }]
    },
    {
      "id": 3, "type": "timeseries", "title": "RPS",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "targets": [{ "expr": "rate(ss_http_request_total[1m])", "refId": "A", "legendFormat": "RPS" }],
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] }
    },
    {
      "id": 4, "type": "timeseries", "title": "HTTP latency p95 (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(ss_http_request_duration_seconds_bucket[5m])) by (le))", "refId": "A", "legendFormat": "p95" }],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] }
    },
    {
      "id": 5, "type": "timeseries", "title": "Memory (RSS)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [{ "expr": "process_resident_memory_bytes", "refId": "A", "legendFormat": "RSS" }],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] }
    },
    {
      "id": 6, "type": "timeseries", "title": "Active coroutines",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [{ "expr": "swoole_coroutines", "refId": "A", "legendFormat": "coroutines" }]
    },
    {
      "id": 7, "type": "bargauge", "title": "Workers (RR-style, approximated)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        { "expr": "ss_http_total_workers",   "refId": "A", "legendFormat": "total" },
        { "expr": "ss_http_workers_ready",   "refId": "B", "legendFormat": "ready" },
        { "expr": "ss_http_workers_working", "refId": "C", "legendFormat": "working" },
        { "expr": "ss_http_workers_invalid", "refId": "D", "legendFormat": "invalid" }
      ]
    },
    {
      "id": 8, "type": "stat", "title": "Queue (alias: in-flight)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 20 },
      "targets": [{ "expr": "ss_http_requests_queue", "instant": true, "refId": "A" }]
    }
  ],
  "templating": { "list": [] }
}